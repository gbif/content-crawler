{
  "_all": {
    "store": true
  },
  "dynamic_templates": [
    {
      "ignored_fields": {
        "match_pattern": "regex",
        "match": "images|space|revision|type",
        "mapping": {
          "include_in_all": false,
          "enabled": false
        }
      }
    },
    {
      "text_fields": {
        "match_pattern": "regex",
        "match": "title|description",
        "mapping": {
          "boost": 10
        }
      }
    },
    {
      "locale": {
        "match": "locale",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "locale": {
        "match": "contentType",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "vocabularies": {
        "match": "vocabulary*",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "vocabularies": {
        "match": "countryOf*",
        "mapping": {
          "type": "keyword"
        }
      }
    },
    {
      "contentType": {
        "match": "contentType",
        "mapping": {
          "type": "keyword"
        }
      }
    }
  ]
}

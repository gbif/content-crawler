{
  "dynamic_templates": [
    {
      "vocabularies": {
        "path_match": "identifiers.*",
        "mapping": {
          "type": "keyword"
        }
      }
    }
  ],
  "properties": {
    "_all": {
      "type": "text"
    },
    "id": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "title": {
      "type": "text",
      "copy_to": "_all",
      "boost": 10
    },
    "literatureType": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "contentType": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "profileId": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "groupId": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "createdAt": {
      "type": "date"
    },
    "updatedAt": {
      "type": "date"
    },
    "abstract": {
      "type": "text",
      "copy_to": "_all"
    },
    "source": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "year": {
      "type": "integer",
      "copy_to": "_all"
    },
    "authors": {
      "type": "nested",
      "properties": {
        "firstName": { "type": "keyword", "copy_to": "_all" },
        "lastName": { "type": "keyword", "copy_to": "_all" }
      }
    },
    "countriesOfResearcher": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "countriesOfCoverage": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "identifiers": {
      "type": "nested"
    },
    "keywords": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "month": {
      "type": "integer"
    },
    "day": {
      "type": "integer"
    },
    "revision": {
      "type": "keyword"
    },
    "pages": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "volume": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "issue": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "websites": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "publisher": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "city": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "edition": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "institution": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "series": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "chapter": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "editors": {
      "type": "nested",
      "properties": {
        "firstName": { "type": "keyword", "copy_to": "_all" },
        "lastName": { "type": "keyword", "copy_to": "_all" }
      }
    },
    "accessed": {
      "type": "date"
    },
    "tags": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "read": {
      "type": "boolean"
    },
    "starred": {
      "type": "boolean"
    },
    "authored": {
      "type": "boolean"
    },
    "confirmed": {
      "type": "boolean"
    },
    "hidden": {
      "type": "boolean"
    },
    "fileAttached": {
      "type": "boolean"
    },
    "citationKey": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "sourceType": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "language": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "shortTitle": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "reprintEdition": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "genre": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "country": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "gbifRegion": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "gbifDatasetKey": {
      "type": "keyword"
    },
    "publishingOrganizationKey": {
      "type": "keyword"
    },
    "gbifDownloadKey": {
      "type": "keyword"
    },
    "translators": {
      "type": "nested",

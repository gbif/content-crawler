      "type": "keyword",
      "copy_to": "_all"
    },
    "userContext": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "department": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "patentOwner": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "patentApplicationNumber": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "patentLegalStatus": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "searchable": {
      "type": "boolean",
      "null_value": true
    },
    "topics": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "relevance": {
      "type": "keyword",
      "copy_to": "_all"
    },
    "peerReview": {
      "type": "boolean",
      "null_value": false
    },
    "openAccess": {
      "type": "boolean",
      "null_value": false
    }
  }
}
